package cse431s.lab3.ast;

public class CharLiteralNode extends BaseASTNode {
    private char value;
    
    public CharLiteralNode(char value) {
        super("CharLiteral");
        this.value = value;
    }
    
    public char getValue() {
        return value;
    }
    
    @Override
    public String toString() {
        return getNodeType() + " (" + value + ")";
    }
}
