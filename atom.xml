---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Galen Collins Homeworks</title>
 <link href="http://bashwork.github.com/school/atom.xml" rel="self"/>
 <link href="http://bashwork.github.com/school/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://bashwork.github.com/school/</id>
 <author>
   <name>Galen Collins</name>
   <email>bashwork@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://bashwork.github.com/school/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://bashwork.github.com/school/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
